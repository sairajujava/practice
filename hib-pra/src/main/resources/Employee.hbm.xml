<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">
<hibernate-mapping>
	<class name="com.entity.Employee" table="employee">
		<id name="id" column="e_id">
			<generator class="assigned"></generator>

		</id>

		<property name="name" column="e_name" />
		<property name="firstName" column="e_firstname" />
		<property name="lastName" column="e_lastname" />
		<property name="salary" column="e_sal" />

	</class>
</hibernate-mapping>